<%- layout('layouts/boilerplate') -%>

<h3>edit places</h3>
<form method="post" action="/listings/<%= listing._id %>?_method=PUT">
  <input name="listings[title]" value="<%= listing.title %> " type="text" />
  <br /><br />

  <textarea name="listings[description]"><%= listing.description %></textarea>
  <br /><br />
  <input name="listings[image]" value="<%= listing.image %>" type="text" />
  <br /><br />
  <input name="listings[price]" value="<%= listing.price %>" type="number" />
  <br /><br />
  <input name="listings[country]" value="<%= listing.country %>" type="text" />
  <br /><br />
  <input
    name="listings[location]"
    value="<%= listing.location %>"
    type="text"
  />
  <br /><br />
  <button>update changes</button>
</form>
